{
  "name": "web-spider",
  "version": "2.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.2",
  "license": "MIT",
  "engines": {
    "node": ">=20.9.0",
    "pnpm": ">=9.0.2"
  },
  "scripts": {
    "postinstall": "npm run gen-env-types",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "npm run start",
    "dev:debug": "npm run start:debug",
    "repl": "npm run start -- --entryFile repl",
    "bundle": "rimraf out && npm run build && ncc build dist/main.js -o out -m -t && chmod +x out/index.js",
    "start": "cross-env NODE_ENV=development nest start -w --path tsconfig.json",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "prod": "cross-env NODE_ENV=production pm2-runtime start ecosystem.config.js",
    "prod:pm2": "cross-env NODE_ENV=production pm2 restart ecosystem.config.js",
    "prod:stop": "pm2 stop ecosystem.config.js",
    "prod:debug": "cross-env NODE_ENV=production nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "doc": "compodoc -p tsconfig.json -s",
    "gen-env-types": "npx tsx scripts/genEnvTypes.ts || true",
    "cleanlog": "rimraf logs"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/multipart": "^8.2.0",
    "@fastify/static": "^7.0.3",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.3.8",
    "@nestjs/platform-socket.io": "^10.3.8",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.8",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "@xmldom/xmldom": "github:xmldom/xmldom",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "bull": "^4.12.2",
    "cache-manager": "^5.5.1",
    "cache-manager-ioredis-yet": "^2.0.3",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crawlee": "^3.9.2",
    "cron": "^3.1.7",
    "cron-parser": "^4.9.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "dotenv": "16.4.5",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "nanoid": "^2.1.11",
    "nest-puppeteer": "^1.1.1",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-headerapikey": "^1.2.2",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "puppeteer": "^22.6.5",
    "qiniu": "^7.11.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "stacktrace-js": "^2.0.2",
    "systeminformation": "^5.22.7",
    "ua-parser-js": "^1.0.37",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "@compodoc/compodoc": "^1.1.23",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/cache-manager": "^4.0.6",
    "@types/jest": "29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/multer": "^1.4.11",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^20.12.7",
    "@types/passport": "^1.0.16",
    "@types/passport-http": "^0.3.11",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/ua-parser-js": "^0.7.39",
    "cross-env": "^7.0.3",
    "eslint": "^9.1.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "simple-git-hooks": "^2.11.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "pnpm": {
    "overrides": {
      "@liaoliaots/nestjs-redis": "npm:@songkeys/nestjs-redis@10.0.0"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
